export WM=9wm
export PLAN9=$HOME/plan9
export BROWSER=chromium
export MAX_BRIGHTNESS=$(cat /sys/class/backlight/*/max_brightness)
export BRIGHTNESS_STEP=$(echo $(($MAX_BRIGHTNESS/20)))
export TERM=uxterm
export PATH=$PATH:/sbin:/usr/sbin:/usr/bin:/usr/local/go/bin:$HOME/go/bin:$PLAN9/bin

eval $(ssh-agent -s);ssh-add /home/utt/.ssh/git_key /home/utt/.ssh/git_key_dRX0
